{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "es6",
      "dom"
    ],
    "paths": {
      "jslib/*": [
        "jslib/src/*"
      ]
    },
    "baseUrl": ".",
    "module": "es6",
    "allowJs": false,
    "checkJs": false,
    "outDir": "out",
    "strict": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "moduleResolution": "node",
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": [
      "./src/types/"
    ]
  },
  "exclude": [
    "node_modules/**/*.ts"
  ]
}